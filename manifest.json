{
    "manifest_version" : 3,
    "name" : "adhd reader",
    "version" : "1.0.0",
    "description" : "extension to help people with adhd read lots of words",
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["script.js"]
        }
    ],
    "action" : {
        "default_popup" : "popup.html"
    },
    "permissions" : [
        "tabs",
        "activeTab",
        "scripting",
        "storage"
    ]
}